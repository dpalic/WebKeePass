<SCRIPT SCRIPT_NAME="KeePass030" DOCUMENT_TYPE="Window" CLASS_NAME="server.Crypt" AUTO_REFRESH="true" ICON="WKP2.jpg" DESCRIPTION="Contraseña Registro de acceso" Screen_Title="Password Access Log" WIDTH="890" HEIGHT="600">
  <ButtonBar>
    <BarButton TYPE="FIND" DESCRIPTION="Buscar / Actualizar" ICON="iFind.gif" METHOD="getAccesslog" />
  </ButtonBar>
  <Panel POSITION="Left" HIDDEN="N">
    <TextField FIELDNAME="PasswordID" DESCRIPTION="Contraseña ID #" LENGTH="10" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="20" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" LINK="KeePass006" METHOD="initAkinList" />
    <TextField FIELDNAME="a0" LENGTH="30" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" />
  </Panel>
  <Panel DESCRIPTION="Búsqueda de valores" POSITION="Left Fill" HIDDEN="N">
    <TextField FIELDNAME="@cc" DESCRIPTION="Usuario" LENGTH="20" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="30" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
    <TextField FIELDNAME="@dd" DESCRIPTION="Dirección IP" LENGTH="20" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="20" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
    <TextField FIELDNAME="@ee" DESCRIPTION="Fecha del Cambio" LENGTH="20" DECIMALS="0" DATA_TYPE="DAT" DATA_LEN="20" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
  </Panel>
  <Table FIELDNAME="Table1" HEIGHT="12" WIDTH="0" POSITION="Left">
    <TableTextField FIELDNAME="AccessID" DESCRIPTION="ID" LENGTH="5" DECIMALS="0" DATA_TYPE="INT" DATA_LEN="20" PROTECTED="Y" ZERO_FILL="N" ENCRYPT="N" ALIGNMENT="Center" />
    <TableTextField FIELDNAME="cc" DESCRIPTION="ID de usuario" LENGTH="8" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="40" PROTECTED="Y" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
    <TableTextField FIELDNAME="dd" DESCRIPTION="IP Addr" LENGTH="9" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="40" PROTECTED="Y" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Center" />
    <TableTextField FIELDNAME="ee" DESCRIPTION="Fecha" LENGTH="12" DECIMALS="0" DATA_TYPE="DAT" DATA_LEN="40" PROTECTED="Y" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Center" />
    <TableTextField FIELDNAME="gg" DESCRIPTION="Tiempo" LENGTH="5" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="12" PROTECTED="Y" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Center" />
    <TableTextField FIELDNAME="ff" DESCRIPTION="Pantalla" LENGTH="10" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="12" PROTECTED="Y" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Center" />
  </Table>
</SCRIPT>

