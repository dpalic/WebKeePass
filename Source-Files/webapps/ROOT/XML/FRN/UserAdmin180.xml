<SCRIPT SCRIPT_NAME="UserAdmin180" DOCUMENT_TYPE="Window" CLASS_NAME="webBoltOns.server.DevelopmentTools" AUTO_REFRESH="false" DESCRIPTION="Générer Source Java de base" Screen_Title="Generate Base Java Source " WIDTH="700" HEIGHT="546">
  <ButtonBar>
    <BarButton TYPE="CLEAR" DESCRIPTION="Clear" ICON="iClear.gif" />
    <BarButton TYPE="POST" DESCRIPTION="Créer un fichier source Java" METHOD="createJavaSourceFile" ICON="iEdit.gif" />
  </ButtonBar>
  <Panel POSITION="Left" Hidden="N">
    <TextField DESCRIPTION="Nom de la classe" FIELDNAME="ClassName" LENGTH="22" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Left" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" />
    <TextField DESCRIPTION="Description" FIELDNAME="Description" LENGTH="50" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Left" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" />
    <TextField DESCRIPTION="Chemin Source" FIELDNAME="SourcePath" LENGTH="50" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Left" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" />
  </Panel>
  <TabGroup HEIGHT="0" WIDTH="0" POSITION="Left" COMPONENT_ORIENTATION="Top">
    <TabItem DESCRIPTION="Base de données sur le tableau">
      <Panel DESCRIPTION="Nom de la table" POSITION="Left Fill" Hidden="N">
	<TextField DESCRIPTION="Nom de la table" FIELDNAME="TableName" LENGTH="20" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Left" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" />
	<TextField DESCRIPTION="Key Field" FIELDNAME="KeyField1" LENGTH="15" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Left" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" />
	<ComboBox FIELDNAME="KeyType" LENGTH="6" DATATYPE="CHR">
	  <ComboItem DESCRIPTION="CHR" />
	  <ComboItem DESCRIPTION="INT" />
	  <ComboItem DESCRIPTION="FLT" />
	</ComboBox>
      </Panel>
      <Panel DESCRIPTION="Description du tableau" POSITION="Left" Hidden="N">
	<TextArea FIELDNAME="TableDescription" HEIGHT="6" WIDTH="20" POSITION="Left" PROTECTED="N" />
      </Panel>
    </TabItem>
    <TabItem DESCRIPTION="Fichier d options Source">
      <CheckBox DESCRIPTION="Détails méthode Get" FIELDNAME="CreateGetMethod" POSITION="Left" />
      <TextField FIELDNAME="GetMethodComment" LENGTH="35" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Right" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" ALIGNMENT="Left" />
      <CheckBox DESCRIPTION="Détails méthode Get Next" FIELDNAME="CreateGetNextMethod" POSITION="Left" />
      <TextField FIELDNAME="GetNextMethodComment" LENGTH="35" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Right" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" ALIGNMENT="Left" />
      <CheckBox DESCRIPTION="Détails méthode Get Préc" FIELDNAME="CreateGetPrevMethod" POSITION="Left" />
      <TextField FIELDNAME="GetPrevMethodComment" LENGTH="35" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Right" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" ALIGNMENT="Left" />
      <CheckBox DESCRIPTION="Méthode de mise à jour détaillée" FIELDNAME="CreateUpdateMethod" POSITION="Left" />
      <TextField FIELDNAME="UpdateMethodComment" LENGTH="35" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Right" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" ALIGNMENT="Left" />
      <CheckBox DESCRIPTION="Supprimer détail Méthode" FIELDNAME="CreateDeleteMethod" POSITION="Left" />
      <TextField FIELDNAME="DeleteMethodComment" LENGTH="35" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Right" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" ALIGNMENT="Left" />
      <CheckBox DESCRIPTION="Méthode Tableau Liste" FIELDNAME="CreateTableListMethod" POSITION="Left" />
      <TextField FIELDNAME="TableListMethodComment" LENGTH="35" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Right" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" ALIGNMENT="Left" />
      <CheckBox DESCRIPTION="Méthode de mise à jour le tableau" FIELDNAME="CreateTableUpdateMethod" POSITION="Left" />
      <TextField FIELDNAME="TableUpdateMethodComment" LENGTH="35" DECIMALS="0" DATATYPE="CHR" DATA_LEN="0" POSITION="Right" PROTECTED="N" Hidden="N" ZERO_FILL_VALUE="N" ALIGNMENT="Left" />
    </TabItem>
    <TabItem DESCRIPTION="Commentaires tête Source">
      <TextArea FIELDNAME="Comments" HEIGHT="11" WIDTH="20" POSITION="Left" PROTECTED="N" />
    </TabItem>
  </TabGroup>
</SCRIPT>

