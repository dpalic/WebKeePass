<SCRIPT SCRIPT_NAME="KeePass003" DOCUMENT_TYPE="Window" CLASS_NAME="server.TheCrypt" AUTO_REFRESH="false" DESCRIPTION="View History" Screen_Title="View History" WIDTH="650" HEIGHT="580">
  <ButtonBar>
    <BarButton TYPE="FIND" DESCRIPTION="Find/Refresh" ICON="iFind.gif" METHOD="getCrypHistory" />
    <BarButton TYPE="PREV" DESCRIPTION="Previous" ICON="iPrev.gif" METHOD="prvCrypHistory" />
    <BarButton TYPE="NEXT" DESCRIPTION="Next" ICON="iNext.gif" METHOD="nxtCrypHistory" />
    <BarButton TYPE="POST" DESCRIPTION="Restore This Item" ICON="iEdit.gif" METHOD="restoreCrypt" />
  </ButtonBar>
  <Panel POSITION="Left" HIDDEN="N">
    <TextField FIELDNAME="HistoryID" DESCRIPTION="HistoryID" LENGTH="4" DECIMALS="0" DATA_TYPE="INT" DATA_LEN="5" POSITION="Left" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" LINK="KeePass001" METHOD="getPassword" />
    <TextField FIELDNAME="PasswordID" DESCRIPTION="PasswordID" LENGTH="4" DECIMALS="0" DATA_TYPE="INT" DATA_LEN="5" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" LINK="KeePass001" METHOD="getPassword" />
    <TextField FIELDNAME="a0" DESCRIPTION="Title" LENGTH="30" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" />
  </Panel>
  <TabGroup HEIGHT="0" WIDTH="0" POSITION="Left" COMPONENT_ORIENTATION="Top">
    <TabItem DESCRIPTION="Password Entry" ICON="tdot.gif">
      <Panel POSITION="Left" HIDDEN="N">
	<TextField FIELDNAME="GroupID" DESCRIPTION="Group" LENGTH="30" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
	<TextField FIELDNAME="b9" DESCRIPTION="User Name" LENGTH="30" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
	<PasswordEditor FIELDNAME="c8" DESCRIPTION="Password" LENGTH="25" DATA_LEN="50" POSITION="Left" HIDDEN="N" ENCRYPT="Y" ALIGNMENT="Left" />
	<TextField FIELDNAME="d7" DESCRIPTION="Create Date" LENGTH="12" DECIMALS="0" DATA_TYPE="DAT" DATA_LEN="20" POSITION="Left" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
	<TextField FIELDNAME="e6" DESCRIPTION="Last Update" LENGTH="12" DECIMALS="0" DATA_TYPE="DAT" DATA_LEN="20" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
	<TextField FIELDNAME="f5" DESCRIPTION="Expires" LENGTH="12" DECIMALS="0" DATA_TYPE="DAT" DATA_LEN="20" POSITION="Left" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
	<CheckBox FIELDNAME="g4" DESCRIPTION="Never Expires" POSITION="Left" ENCRYPT="Y" />
      </Panel>
      <Panel POSITION="Left" HIDDEN="N">
	<TextField FIELDNAME="n7" DESCRIPTION="Web Site" LENGTH="40" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
	<TextField FIELDNAME="i2" DESCRIPTION="Alt Web Site" LENGTH="40" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
	<TextField FIELDNAME="k0" DESCRIPTION="Support Site" LENGTH="40" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
	<TextField FIELDNAME="m8" DESCRIPTION="Email" LENGTH="40" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="Y" HIDDEN="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Left" />
      </Panel>
    </TabItem>
    <TabItem DESCRIPTION="Notes" ICON="tdot.gif">
      <TextRTFArea FIELDNAME="Notes" DESCRIPTION=" Notes" HEIGHT="10" WIDTH="15" POSITION="Left" PROTECTED="N" />
    </TabItem>
    <TabItem DESCRIPTION="Image/Icon" ICON="tdot.gif">
      <ImageBox FIELDNAME="Image" LENGTH="20" HEIGHT="10" WIDTH="0" POSITION="Left" PROTECTED="N" />
    </TabItem>
  </TabGroup>
</SCRIPT>

