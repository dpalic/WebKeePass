	</Panel>
      </Panel>
    </TabItem>
    <TabItem DESCRIPTION="KeePass Gruppen">
      <Panel FIELDNAME="Bundles" DESCRIPTION="KeePass Gruppen Dieser Benutzer wird in" POSITION="Left Fill" HIDDEN="N">
	<Table FIELDNAME="Table1" HEIGHT="10" WIDTH="15" POSITION="Left Fill">
	  <TableTextField FIELDNAME="cc" DESCRIPTION="KeePass Group" LENGTH="10" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="30" PROTECTED="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Center" LINK="KeePass018" PARAMETER="@aa" />
	  <TableTextField FIELDNAME="dd" DESCRIPTION="Beschreibung" LENGTH="25" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="30" PROTECTED="N" ZERO_FILL="N" ENCRYPT="Y" ALIGNMENT="Center" />
	  <TableCheckBox FIELDNAME="ee" DESCRIPTION="Aktiv" ENCRYPT="Y" />
	</Table>
      </Panel>
    </TabItem>
  </TabGroup>
  <Panel POSITION="Left Fill" HIDDEN="N">
    <Button FIELDNAME="pbndles" DESCRIPTION="Erstellen KeePass Group" POSITION="Left" LINK="KeePass017" METHOD_CLASS="server.Akin" METHOD="getBndList" />
  </Panel>
</SCRIPT>

