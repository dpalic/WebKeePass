    <BarButton TYPE="DELETE" DESCRIPTION="Löschen" ICON="iDelete.gif" METHOD="deleteUser" />
    <BarButton TYPE="PREV" DESCRIPTION="Zurück" ICON="iPrev.gif" METHOD="prvUser" />
    <BarButton TYPE="NEXT" DESCRIPTION="Nächste" ICON="iNext.gif" METHOD="nxtUser" />
    <BarButton TYPE="POST" DESCRIPTION="Add / Update" ICON="iEdit.gif" METHOD="updUser" />
    <BarButton TYPE="NEW-LINE" DESCRIPTION="New KeePass Grp" ICON="iNew.gif" />
    <BarButton TYPE="DEL-LINE" DESCRIPTION="Löschen KeePass Grp" ICON="iNew.gif" METHOD="deleteAdherent" />
  </ButtonBar>
  <Panel HIDDEN="N">
    <TextField FIELDNAME="UserID" DESCRIPTION="User-ID" LENGTH="8" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="0" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" LINK="UserAdmin130" METHOD="getUser" />
    <TextField FIELDNAME="UserDescription" DESCRIPTION="Beschreibung" LENGTH="20" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="0" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
    <PasswordField FIELDNAME="Password" DESCRIPTION="Kennwort" LENGTH="20" DATA_LEN="30" POSITION="Left" HIDDEN="N" ENCRYPT="N" ALIGNMENT="Left" />
    <CheckBox FIELDNAME="UpdPW" DESCRIPTION="Update Passwort" ENCRYPT="N" />
  </Panel>
  <Panel POSITION="Left" HIDDEN="N">
    <TextField FIELDNAME="WebSite" DESCRIPTION="Web Site" LENGTH="47" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="100" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
    <Button FIELDNAME="WebSite" METHOD="URL" />
    <TextField FIELDNAME="Email" DESCRIPTION="E-Mail Adresse" LENGTH="47" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="100" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
    <Button FIELDNAME="Email" ICON="webEMAIL.gif" METHOD="EMAIL" />
  </Panel>
  <TabGroup HEIGHT="0" WIDTH="0" POSITION="Left" COMPONENT_ORIENTATION="Bottom">
    <TabItem DESCRIPTION="Adresse">
      <Panel DESCRIPTION="Adresse Information" POSITION="Left Fill" HIDDEN="N">
	<TextField FIELDNAME="Name" DESCRIPTION="Name" LENGTH="25" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
	<TextField FIELDNAME="Address1" DESCRIPTION="Adresse" LENGTH="30" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
	<TextField FIELDNAME="Address2" LENGTH="30" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
	<TextField FIELDNAME="Address3" LENGTH="30" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
	<TextField FIELDNAME="Address4" LENGTH="20" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
	<TextField FIELDNAME="Phone1" DESCRIPTION="Telefonnummer" LENGTH="25" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
	<TextField FIELDNAME="Fax" DESCRIPTION="Faxnummer" LENGTH="25" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="50" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
      </Panel>
      <Panel DESCRIPTION="Information Access" POSITION="Right Fill" HIDDEN="N">
	<Panel HIDDEN="N">
	  <CheckBox FIELDNAME="ActiveUser" DESCRIPTION="Active User" POSITION="Left" ENCRYPT="N" />
	</Panel>
	<Panel HIDDEN="N">
	  <TextField FIELDNAME="GroupID" DESCRIPTION="User Group" LENGTH="10" DECIMALS="0" DATA_TYPE="CHR" DATA_LEN="0" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" LINK="UserAdmin120" PARAMETER="GroupID" />
	  <TextField FIELDNAME="LastChangeDate" DESCRIPTION="Letztes Update" LENGTH="9" DECIMALS="0" DATA_TYPE="DAT" DATA_LEN="0" POSITION="Left" PROTECTED="N" HIDDEN="N" ZERO_FILL="N" ENCRYPT="N" />
